[user]
    name = David Hazinski
    email = dhazinski@gmail.com
[color]
    ui = auto
[color "branch"]
    current = green reverse
    local = green
    remote = yellow
[color "diff"]
    meta = yellow
    frag = magenta
    old = red
    new = green
[color "status"]
    added = green
    changed = yellow
    untracked = red
[advice]
    statusHints = false
[diff]
    renames = copies
[core]
    whitespace=fix,tabwidth=4,tab-in-indent,space-before-tab,trailing-space,cr-at-eol
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
